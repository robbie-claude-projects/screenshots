# Ad Placement Visualization Tool - Environment Configuration
# Copy this file to .env and modify as needed

# Server Configuration
PORT=3000
NODE_ENV=development

# Screenshot Settings
SCREENSHOT_DIR=./screenshots
MAX_CONCURRENT=3
TIMEOUT_MS=60000

# Cleanup Settings
CLEANUP_HOURS=24
ENABLE_AUTO_CLEANUP=true

# Image Quality (0-100, only applies to JPEG)
IMAGE_QUALITY=90

# Retry Settings
MAX_RETRIES=1
RETRY_DELAY_MS=5000
